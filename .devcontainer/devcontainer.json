{
  "name": "FastAPI Dev",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "api",
  "workspaceFolder": "/workspace",
  "forwardPorts": [8000, 5432],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "cweijan.vscode-database-client2"
      ]
    }
  },
  "containerEnv": {
      "DISPLAY": "dummy",
      "PYTHONUNBUFFERED": "True",
      "UV_LINK_MODE": "copy",
      "UV_PROJECT_ENVIRONMENT": "/home/vscode/.venv"
  },
  "remoteUser": "vscode"
}