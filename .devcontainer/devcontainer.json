{
  "name": "Mi casa en minutos",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "features": {
		"ghcr.io/rails/devcontainer/features/postgres-client:1": {}
	},
  "service": "develop",
  "workspaceFolder": "/workspace",
  "forwardPorts": [8000, 5432, 8080],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "cweijan.vscode-database-client2",
        "vue.volar",
        "mariorodeghiero.vue-theme"
      ]
    }
  },
  "containerEnv": {
      "DISPLAY": "dummy",
      "PYTHONUNBUFFERED": "True",
      "UV_LINK_MODE": "copy",
      "UV_PROJECT_ENVIRONMENT": "/home/vscode/.venv"
  },
  "remoteUser": "vscode"
}